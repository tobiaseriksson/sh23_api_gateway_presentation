    {
    "version": 3,
    "name": "SH23 API Gateway",
    "port": 8080,
    "cache_ttl": "3600s",
    "timeout": "30s",
    "extra_config": {
        "router": {
            "return_error_msg": true
        },
        "telemetry/logging": {
            "level": "DEBUG",
            "prefix": "[KRAKEND]",
            "syslog": false,
            "stdout": true
        },
        "telemetry/metrics": {
            "collection_time": "30s",
            "listen_address": ":8090"
        },
        "telemetry/influx": {
            "address": "http://influxdb:8086",
            "ttl": "25s",
            "buffer_size": 100,
            "username": "krakend-dev",
            "password": "pas5w0rd"
        }
    },
    "endpoints": [
        {
            "endpoint": "/sh23/api/individuals",
            "method": "GET",
            "output_encoding": "json-collection",
            "input_headers": [
                "*"
            ],
            "backend": [
                {
                    "host": [
                        "http://individuals_api_1:5059"
                    ],
                    "url_pattern": "/api/v1/individuals",
                    "method": "GET",
                    "is_collection": true,
                    "extra_config": {
                        "backend/http": {
                            "return_error_code": true
                        }
                    }
                }
            ]
        },
        {
            "endpoint": "/sh23/api/individuals/{id}",
            "method": "GET",
            "extra_config": {
                "qos/ratelimit/router": {
                    "max_rate": 50,
                    "every": "1s"
                }
            },
            "output_encoding": "json",            
            "input_headers": [
                "*"
            ],
            "backend": [
                {
                    "host": [
                        "http://individuals_api_1:5059"
                    ],
                    "url_pattern": "/api/v1/individuals/{id}",
                    "method": "GET",
                    "encoding": "json",
                    "extra_config": {
                        "backend/http": {
                            "return_error_code": true
                        }
                    }
                }
            ]
        },
        {
            "endpoint": "/sh23/api/addresses",
            "method": "GET",
            "output_encoding": "json-collection",
            "input_headers": [
                "Authorization",
                "Content-Type"
            ],
            "backend": [
                {
                    "host": [
                        "http://address_api_1:5059"
                    ],
                    "url_pattern": "/api/v1/addresses",
                    "method": "GET",
                    "is_collection": true,
                    "extra_config": {
                        "backend/http": {
                            "return_error_code": true
                        }
                    }
                }
            ]
        },
        {
            "endpoint": "/sh23/api/addresses/{id}",
            "method": "GET",
            "input_headers": [
                "Authorization",
                "Content-Type"
            ],
            "backend": [
                {
                    "host": [
                        "http://address_api_1:5059"
                    ],
                    "url_pattern": "/api/v1/addresses/{id}",
                    "method": "GET",
                    "extra_config": {
                        "backend/http": {
                            "return_error_code": true
                        }
                    }
                }
            ]
        }
    ]
}